{
  "name": "sola_template",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "pnpm -r run type-check",
    "lint:web": "pnpm --filter @sola/web lint",
    "check:web": "pnpm --filter @sola/web check && pnpm check:boundary",
    "check:boundary": "node scripts/check-trpc-jotai-boundary.mjs && node scripts/check-feature-boundary.mjs && node scripts/check-atom-privacy.mjs",
    "type-check": "turbo run type-check",
    "typecheck": "pnpm -r typecheck",
    "tts:sync:azure": "node scripts/sync-azure-voices.mjs",
    "db:seed:ai": "node scripts/seed-public-ai.mjs",
    "db:seed:ai-instruction": "node scripts/seed-public-ai-instruction.mjs",
    "db:seed:tts": "node scripts/seed-public-tts.mjs",
    "db:seed:ai-json": "node scripts/seed-public-ai-from-json.mjs",
    "db:export:ai": "node scripts/export-public-ai-data.mjs"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "devDependencies": {
    "turbo": "^2.6.3",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0"
  }
}
